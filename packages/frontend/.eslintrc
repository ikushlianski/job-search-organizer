{
        "extends": [
            "react-app",
            "plugin:react/recommended",
            "../../.eslintrc"
        ],
        "plugins": ["react-hooks", "import"],
        "overrides": [
            {
                "files": [
                    "**/*.ts?(x)"
                ],
                "rules": {
                    "react-hooks/rules-of-hooks": "warn",
                    "react-hooks/exhaustive-deps": "warn",
                    "react/prop-types": "off"
                }
            }
        ]
}
