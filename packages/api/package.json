{
  "name": "@job-search-organizer/api",
  "version": "0.1.0",
  "description": "Job search organizer API",
  "author": "Ilya Kushlianski <kushliansky@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "main": "src/index.ts",
  "scripts": {
    "start": "NODE_PATH=src node --inspect=5858 -r dotenv/config -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "db:create": "env-cmd -f .env sequelize db:create",
    "db:seed": "env-cmd -f .env sequelize db:seed:all",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1",
    "pg": "8.0.0",
    "pg-hstore": "2.3.3",
    "sequelize": "5.21.5",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@job-search-organizer/common": "*",
    "@types/express": "4.17.3",
    "@types/node": "13.9.2",
    "@types/sequelize": "4.28.8",
    "@typescript-eslint/parser": "2.25.0",
    "env-cmd": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "nodemon": "2.0.2",
    "prettier": "2.0.2",
    "sequelize-cli": "5.5.1",
    "ts-node": "8.8.1",
    "tsconfig-paths": "3.9.0"
  },
  "peerDependencies": {
    "@job-search-organizer/common": "0.0.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
