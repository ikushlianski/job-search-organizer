{
  "name": "api",
  "version": "0.1.0",
  "description": "Job search organizer API",
  "author": "Ilya Kushlianski <kushliansky@gmail.com>",
  "homepage": "",
  "license": "ISC",
  "main": "src/index.ts",
  "scripts": {
    "start": "node --inspect=5858 -r dotenv/config -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "test": "echo \"Error: run tests from root\" && exit 1"
  },
  "dependencies": {
    "body-parser": "1.19.0",
    "dotenv-safe": "8.2.0",
    "express": "4.17.1",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/express": "4.17.3",
    "@types/node": "13.9.2",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "nodemon": "2.0.2",
    "prettier": "2.0.2",
    "ts-node": "8.8.1"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "yarn start",
    "ext": "ts"
  }
}
